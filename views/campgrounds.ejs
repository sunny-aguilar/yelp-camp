<!-- header -->
<%- include('partials/header') %>


<!-- content -->
<div class="container">
    <header class="jumbotron">
        <h1>Campground Locations</h1>
        <p>Welcome to campgrounds located in northern California.</p>
        <p></p><a class="btn btn-primary" href="/campgrounds/new">Add New Campground</a></p>
    </header>
    <div class="row">
        <div class="col-lg-12">
            <h3>California's Popular Camping Grounds</h3>
        </div>
    </div>
    <% campgrounds.forEach(function(campground) { %>
        <div>
            <h3><%= campground.name %></h3>
            <img src="<%= campground.image %>">
        </div>
    <% }); %>
</div>

<!-- footer -->
<%- include('partials/footer') %>